	"item": [
		{
			"name": "CT001 - Cadastrar Usuário Comum Válido",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"pm.test(\"Response time is less than 200ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"});\r",
							"const schema = {\r",
							"  \"$schema\": \"http://json-schema.org/draft-07/schema#\",\r",
							"  \"title\": \"Schema de Resposta de Sucesso\",\r",
							"  \"description\": \"Schema para uma resposta padrão de sucesso da API após a criação de um novo recurso.\",\r",
							"  \"type\": \"object\",\r",
							"  \"properties\": {\r",
							"    \"message\": {\r",
							"      \"description\": \"Mensagem de feedback para o usuário, indicando o resultado da operação.\",\r",
							"      \"type\": \"string\",\r",
							"      \"example\": \"Cadastro realizado com sucesso\"\r",
							"    },\r",
							"    \"_id\": {\r",
							"      \"description\": \"O identificador único do recurso que foi criado.\",\r",
							"      \"type\": \"string\",\r",
							"      \"example\": \"jogfODIlXsqxNFS2\"\r",
							"    }\r",
							"  },\r",
							"  \"required\": [\r",
							"    \"message\",\r",
							"    \"_id\"\r",
							"  ]\r",
							"};\r",
							"pm.test(\"Validação do corpo da resposta\", function () {\r",
							"    pm.response.to.have.jsonSchema(schema);\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"nome\": \"Usuário Teste\",\r\n  \"email\": \"usuario.teste@example.com\",\r\n  \"password\": \"senha123\",\r\n  \"administrador\": \"false\"\r\n}",
